<?xml version="1.0" encoding="UTF-8" ?>
<odoo>



    <record id="fullfilemt_stage_action" model="ir.actions.act_window">
        <field name="name">Fullfilemt Stage</field>
        <field name="res_model">fullfilemt.stage</field>
        <field name="view_type">form</field>
        <field name="view_mode">list</field>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new
            </p>
        </field>
    </record>

    <record id="fullfilemt_multi_tree" model="ir.ui.view">
        <field name="name">fullfilemt.multi.tree</field>
        <field name="model">fullfilemt.multi</field>
        <field name="arch" type="xml">
            <tree >
                <field name="name"/>
                <field name="stage_id"/>
            </tree>
        </field>
    </record>

    <record id="fullfilemt_multi_action" model="ir.actions.act_window">
        <field name="name">Fullfilemt Multi</field>
        <field name="res_model">fullfilemt.multi</field>
        <field name="view_type">form</field>
        <field name="view_mode">list</field>
        <field name="target">main</field>
    </record>
    


</odoo>